module MODIFIERS-SYNTAX
	
	syntax Modifier ::= "private" 
                   	| "public" 
                    | "protected" 
                    | "default"
   
  syntax Abst ::= "abstract" 
                | "nonabstract"

  syntax Final ::= "final" 
                  | "nonfinal"
          
  syntax Static ::= "static"  
                  | "nonstatic"
endmodule